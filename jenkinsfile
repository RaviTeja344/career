pipeline{
    agent any
    tools{
        maven 'maven-3.9.4'
    }
    stages{
        stage('Build Maven'){
        steps{
            checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/RaviTeja344/career']])
            sh 'mvn clean install'
        }
      }
    stage("Build Docker Image"){
    steps{
    script{
          sh 'docker build -t product-service .'
    }
    }
}
}
}